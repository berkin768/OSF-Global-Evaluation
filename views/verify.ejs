<html>
<head>
  <link rel="stylesheet" type="text/css" href="/css/bootstrap.css">
  <link rel="stylesheet" type="text/css" href="/css/index.css">
  <script src="/js/jquery-3.3.1.min.js" type="text/javascript"></script>
  <script src="/js/angular.js" type="text/javascript"></script>
  <script src="/js/bootstrap.min.js" type="text/javascript"></script>
  <script src="/js/indexHandler.js" type="text/javascript"></script>
  <script src="/js/userFactory.js" type="text/javascript"></script>
  <title>
    OSF GLOBAL
  </title>
</head>

<body ng-app="index" ng-controller="indexController">
  <header>
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <a class="navbar-brand" href="/" style="padding-top:3px">
            <img width="200" height="40" src="/images/logo.png" style="float:left">
          </a>
        </div>
        <div id='navbar' class="navbar-collapse collapse">
          <ul class="nav navbar-nav">            
              <li ng-repeat="category in categories">
                    <a href="{{category.url}}">{{category.text}}</a>
              </li>
          </ul>
          <ul class="nav navbar-nav navbar-right" >
              <li ng-repeat="url in objects">
                  <a ng-if="url.type === 'unAuth'" href='{{url.url}}'>{{url.text}}</a>
               </li>
          </ul>
        </div>
      </div>
    </nav>
  </header>

  <div id='modal' class="modal fade" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <h4 class="modal-title">Verification</h4>
            </div>
            <div class="modal-body">
              <p>User Verified</p>
            </div>
            <div class="modal-footer">
              <a href='/' type="button" class="btn btn-primary">Go Homepage</a>
            </div>
          </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
      </div><!-- /.modal -->
</body>
<script>
    $("#modal").modal()
</script>
</html>